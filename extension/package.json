{
  "name": "boolcheck-browser-extension",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "build": "run-s clean && tsc -b && vite build",
    "build-to-ff": "run-s clean && tsc && vite build && run-s firefox-mv2-build",
    "firefox-mv2-build": "node tools/unstable_mv3ToMv2Build.js",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "clean:dist-firefox-v2": "rimraf dist-firefox-v2",
    "format": "run-p format:*",
    "format:eslint": "run-s \" lint:eslint --fix \"",
    "format:prettier": "run-s \" lint:prettier --write \"",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"**/*.(md|json|yml|ts|tsx)\" --ignore-pattern .gitignore --check",
    "lint:type": "tsc --noEmit",
    "test": "jest",
    "ci:test": "run-s \" test --ci --reporters=\"default\" --reporters=\"github-actions\" \""
  },
  "dependencies": {
    "@eduardoac-skimlinks/webext-redux": "^3.0.1-release-candidate",
    "@reduxjs/toolkit": "^2.2.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loading": "^2.0.3",
    "react-redux": "^9.1.2",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.28",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.2.5",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-chrome": "^0.8.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.11.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.1"
  }
}
